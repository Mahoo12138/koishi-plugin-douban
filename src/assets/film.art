<body>
  <div id="wrapper">
    <div id="content">
      <div class="grid col-container clearfix">
        <div class="fix-col grid-col1">
          <div class="picture-wrapper" style="background-image: url({{data.cover_url}})">
          </div>
        </div>
        <div class="fix-col grid-col2">

          <div class="main-title">
            <h1>{{data.title}}</h1>
          </div>
          <div class="baseinfo">
            {{each info.base}}
              <span>{{$value}}</span>
            {{/each}}
          </div>
        </div>
        <div class="fix-col grid-col3">
          <div class="line-wrap">
            <img
              src="https://img3.doubanio.com/dae/staticng/s/movie/16890517cac492e9c3605fdaa39d60f8898e4340/share/movie/pics/output_card/line.png" />
          </div>
        </div>
        <div class="fix-col grid-col4">
          <div class="douban-icon">
            <span class="icon-pt1">豆</span>
            <span class="icon-pt2">豆瓣评分</span>
          </div>

          <div class="score-container">
            <div class="rating_self clearfix" typeof="v:Rating">
    <strong class="rating_num" property="v:average">7.5</strong>
    <span property="v:best" content="10.0"></span>
    <div class="rating_right">
        <div class="bigstar bigstar40"></div>
        <div class="rating_sum">
                <a href="comments" class="rating_people">
                    <span property="v:votes">{{data.rating.count}}</span>人评价
                </a>
        </div>
    </div>
